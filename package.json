{
  "name": "zer0tv-web",
  "version": "1.0.0",
  "description": "react template with redux",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack -p --config ./config/webpack.config.js --progress",
    "build": "npm run clean && npm run build:webpack && cross-env NODE_ENV=production npm run server",
    "heroku": "npm run server",
    "heroku-postbuild": "webpack -p --config ./config/webpack.config.js --progress",
    "start": "npm run clean && cross-env NODE_ENV=development npm run server",
    "dev": "npm run start",
    "prod": "npm run build:prod && npm run start:prod",
    "staging": "cross-env NODE_ENV=staging npm run server",
    "server": "node server.js",
    "build:prod": "npm run clean && npm run build:webpack",
    "start:prod": "cross-env NODE_ENV=production forever start --colors -t -a -l prod.log -o prod.log -e prod.log --uid zerotv-web ./server.js",
    "stop:prod": "forever stop zerotv-web",
    "restart:prod": "forever restart --colors -t -a -l prod.log -o prod.log -e prod.log --uid zerotv-web ./server.js"
  },
  "author": "Jonathan Gertig <jcgertig@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.22.1",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "classnames": "^2.2.5",
    "color-it": "^1.2.7",
    "compression": "^1.6.2",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "emojic": "^1.1.7",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "fetchum": "^3.2.0",
    "fetchum-redux": "^1.0.1",
    "file-loader": "^0.11.2",
    "forever": "^0.15.3",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template-react": "^1.1.2",
    "json-loader": "^0.5.4",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "precss": "^2.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.2",
    "react-router-dom": "^4.0.0-beta.3",
    "react-select": "1.0.0-rc.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.7",
    "webpack": "^3.0.0",
    "webpack-merge": "^4.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^4.1.1",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "eventsource-polyfill": "^0.9.6",
    "webpack-dev-server": "^2.2.1",
    "webpack-hot-middleware": "^2.16.1"
  }
}
